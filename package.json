{
  "name": "n8n-nodes-chainstream",
  "version": "0.0.88",
  "description": "Chainstream nodes for n8n workflows",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "n8n-community-node-package",
    "chainstream",
    "automation",
    "workflow",
    "web3",
    "data"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chainstream-io/n8n-nodes-chainstream",
  "bugs": {
    "url": "https://github.com/chainstream-io/n8n-nodes-chainstream/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chainstream-io/n8n-nodes-chainstream.git"
  },
  "author": {
    "name": "Chainstream Team",
    "email": "dev@chainstream.io"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && node scripts/generate-schema.js && gulp build:icons",
    "lint": "eslint nodes credentials",
    "test": "jest --passWithNoTests",
    "precheck": "pnpm exec n8n-node prerelease",
    "pack": "pnpm pack",
    "pack:check": "node scripts/pack-check.js",
    "release": "pnpm run build && pnpm run lint && pnpm run test && pnpm run precheck && pnpm pack"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ChainstreamApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Chainstream/Chainstream.node.js",
      "dist/nodes/Chainstream/ChainstreamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.13.0",
    "@n8n/scan-community-package": "^0.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.9.1",
    "@typescript-eslint/parser": "^8.0.0",
    "change-case": "4.1.2",
    "eslint": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.6.3",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "change-case": "4.1.2"
    }
  }
}
