{
  "name": "n8n-nodes-chainstream",
  "version": "0.0.80",
  "description": "Chainstream nodes for n8n workflows",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "n8n-community-node-package",
    "chainstream",
    "automation",
    "workflow",
    "web3",
    "data"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chainstream-io/n8n-nodes-chainstream",
  "bugs": {
    "url": "https://github.com/chainstream-io/n8n-nodes-chainstream/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chainstream-io/n8n-nodes-chainstream.git"
  },
  "author": {
    "name": "Chainstream Team",
    "email": "dev@chainstream.io"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest --passWithNoTests"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ChainstreamApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Chainstream/Chainstream.node.js",
      "dist/nodes/Chainstream/ChainstreamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.13",
    "@typescript-eslint/parser": "~8.36",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "n8n-core": "^1.11.1",
    "n8n-workflow": "^1.70.0"
  },
  "dependencies": {
    "zod": "3.24.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  }
}
