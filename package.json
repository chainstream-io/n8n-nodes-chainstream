{
	"name": "n8n-nodes-chainstream",
	"version": "0.0.40",
	"description": "Chainstream nodes for n8n",
	"keywords": [
		"n8n-community-node-package",
		"chainstream",
		"chainstream-client",
		"chainstream-client-node",
		"chainstream-client-n8n",
		"chainstream-client-n8n-node",
		"chainstream-client-n8n-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "AI",
		"email": "ai@liberfi.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/liberfi-io/n8n-nodes-chainstream.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"test": "jest"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ChainstreamApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Chainstream/Chainstream.node.js",
			"dist/nodes/Chainstream/ChainstreamTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^24.7.0",
		"@typescript-eslint/parser": "~8.36",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^5.0.1",
		"jest": "^29.7.0",
		"n8n-workflow": "*",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.0",
		"typescript": "~5.8.3"
	},
	"dependencies": {
		"change-case": "^5.4.4",
		"zod": "3.24.0",
		"zod-to-json-schema": "^3.24.6"
	},
	"overrides": {
		"pkce-challenge": "3.0.0",
		"zod": "3.24.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
